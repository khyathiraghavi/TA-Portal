.simple table {
	width: 100%;
	border-collapse: separate;
	margin: 0 0 20px;
	margin-top:25px;
	background-color: #FFF;
 }
.slider a  {
	text-decoration: none;
	color: white;
	margin: 0px;
	padding: 0px;
}
.simple a{
	color:black;
}
.simple table thead tr th,
.simple table thead tr td {
	text-align:center;
	display: table-cell;
	vertical-align: bottom;
	padding-bottom: 5px;
	padding-top: 10px;
	padding-left: 5px;
	border-bottom: 1px #ddd solid;
	border-right: 1px #ddd solid;
	border-left: 1px transparent solid;
	border-top: 1px transparent solid;
	font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
	font-weight: 500;
	font-size: 15px;
	letter-spacing: 0.01em;
	line-height: 13pt;
	font-smooth: always;
	color: rgba(0,0,0,1);
	text-align: center;
}
.simple table thead tr th.right,
.simple table thead tr td.right {
  text-align: center;
  padding-right: 10px;
}
.simple table thead tr th.last,
.simple table thead tr td.last {
  border-right: 1px transparent solid;
}
.simple table thead tr th:last-child,
.simple table thead tr td:last-child {
  border-right: 1px transparent solid;
}
.simple table tbody tr {
  border: 1px #fff solid;
}
.simple table tbody tr td {	
	font-family: 'Segoe UI Semilight', 'Open Sans', Verdana, Arial, Helvetica, sans-serif;
	/*font-weight: 500;*/
	font-size: 15px;
	letter-spacing: 0.04em;
	line-height: 14px;
	font-smooth: always;
	padding: 3px 10px;
	border-right: 1px #ddd solid;
	border-bottom: 1px #ddd solid;
	border-left: 1px transparent solid;
	border-top: 1px transparent solid;
	slider-sizing: border-slider;
}
@-moz-document url-prefix() {
    .simple table tbody tr td  {
      font-family: 'Segoe UI', 'Open Sans', Verdana, Arial, Helvetica, "sans-serif;";
      font-weight:200; }
}
.simple table tbody tr td.right {
  text-align: right;
}
.simple table tbody tr td.center {
  text-align: center;
}
.simple table tbody tr td.last {
  border-right: 1px transparent solid;
}
.simple table tbody tr td:last-child {
  border-right: 1px transparent solid;
}
 
.simple table tbody tr.selected-row {
  background-color: rgba(28, 183, 236, 0.1) !important;
}
.simple table tbody tr.selected-row td:first-child {
  border-left: 1px #1c98cc solid;
}
.simple table tbody tr.selected-row td:last-child {
  border-right: 1px #1c98cc solid;
}
.simple table tbody tr.selected-row td {
  border-top: 1px #1c98cc solid;
  border-bottom: 1px #1c98cc solid;
}

